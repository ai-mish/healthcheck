{
"version":"1.0",
"comment":"Health Check",
"enabled":"Yes",
"smtp":{"host":"myhost","port":587,"user":"myuser","password":"mypassword"},
"sendemail":"no",
"log":"/data/projects/sas/healthcheck/githubproject/mylogfile.log",
"status_jinja2_html_template":"/data/projects/sas/healthcheck/githubproject/status.html.template",
"run_interval_seconds":60,
"run_counter":10,
"alert_lifetime":7200,
"verbose":"yes",
"env":
      {
      "name":"Dev",
      "level":"Lev2",
      "enabled":"Yes",
      "services" : [
                            {
                              "enabled":"yes",
                              "type":"WebApp",
                          		"name":"webcheck",
                              "Description":"Dev environment web check",
                              "protocol":"https",
                          		"hosts":"testserver",
                          		"port":2443,
                          		"apps":["SASStoredProcess","SASStudio"],
                              "user":"demo",
                              "password":"password",
                              "debug":"No"
                            },
                            {
                              "enabled":"yes",
                              "type":"disk",
                              "name":"diskcheck",
                              "Description":"Dev environment disk check",
                              "protocol":"ssh",
                              "hosts":["192.168.56.201","192.168.56.202","192.168.56.203"],
                              "port":22,
                              "ssh_private_key_filename":"/vagrant/va73_dist/ssh_keys/id_rsa",
                              "apps":["/opt2","/tmp"],
                              "user":"sas",
                              "password":"password",
                              "debug":"no"
                          }
                     ]
    }
}
