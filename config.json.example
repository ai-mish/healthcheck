{
"version":"1.0",
"comment":"Health Check",
"enabled":"yes",
"interval":120,
"frequency":1,
"log":"/data/projects/sas/healthcheck/githubproject/mylogfile.log",
"env_name":"Dev",
"env_level":"Lev2",
"alert_lifetime":7200,
"smtp":{"host":"myhost","port":587,"user":"myuser","password":"mypassword"},
"email_enabled":"no",
"email_subject":"Dev Service Down Alert",
"jinja2_template":"status.html.template",
"verbose":"yes",
"services":[
            {
              "type":"webapp",
              "description":"Dev environment web check",
              "protocol":"https",
          		"hosts":{"testserver":"SAS Web Application F5 Host Name"},
          		"port":2443,
          		"name":["SASStoredProcess","SASStudio"],
              "user":"demo",
              "password":"password",
              "enabled":"yes",
              "debug":"No"
            },
            {
              "type":"disk",
              "description":"Dev environment disk check",
              "protocol":"ssh",
              "hosts":{
                       "192.168.56.201":"SAS Metadata Server node 1",
                       "192.168.56.202":"SAS Metadata Server node 2",
                       "192.168.56.203":"SAS Grid node 1"
                     },
              "port":22,
              "ssh_id_rsa_filename":"/vagrant/va73_dist/ssh_keys/id_rsa",
              "name":["/opt2","/tmp"],
              "user":"sas",
              "password":"password",
              "enabled":"yes",
              "debug":"no"
          }
     ]
}
